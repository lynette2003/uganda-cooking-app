{
  "recipe_id": "ug_recipe_egg_roll_070",
  "name": {
    "en": "Ugandan Egg Roll",
    "local_name": "Egg Roll / Rolex"
  },
  "description": "A popular and beloved Ugandan street food consisting of a thin, soft omelette rolled inside a freshly made chapati. It's a flavorful, filling, and portable meal.",
  "origin_region": "Nationwide",
  "cultural_significance": {
    "importance": "iconic_street_food",
    "occasions": ["breakfast", "quick_lunch", "street_food", "university_campuses", "road_trips"],
    "cultural_notes": "The name 'Rolex' comes from a playful contraction of 'Rolled Eggs'. It is one of Uganda's most famous culinary exports and a staple for students, workers, and travelers needing a quick, affordable, and delicious meal. It's a symbol of Ugandan street food ingenuity."
  },
  "type": "main_course_breakfast",
  "difficulty": "medium",
